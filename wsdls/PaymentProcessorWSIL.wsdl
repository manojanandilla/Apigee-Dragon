          <xsd:element name="answerURLTitle" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>URL title to the answer</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="answerURLLink" type="csom:ReferenceURI" minOccurs="0">
