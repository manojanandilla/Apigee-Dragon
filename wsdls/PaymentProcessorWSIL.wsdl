          <xsd:enumeration value="SMS"/>
          <xsd:enumeration value="MAIL"/>
          <xsd:enumeration value="PHONE"/>
          <xsd:enumeration value="PUSHNOTIFICATION"/>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="ClaimReference">
