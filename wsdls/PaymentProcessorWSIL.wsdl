          </xsd:element>
          <xsd:element name="actionName" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of the action granted or denied on the resource. For example: view, modify, delete, etc.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="BaseResponse" abstract="true">
        <xsd:annotation>
          <xsd:documentation>The basic type that should be extended for a response.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="base:BaseRequest">
