      <xsd:complexType name="ResponseStatus">
        <xsd:sequence>
          <xsd:element name="detail" type="base:ResponseStatusDetail" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>
    						Status details that describe one or more contributions to the overall response code.
    					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="code" type="base:ResponseStatusCode"/>
      </xsd:complexType>
      <xsd:complexType name="ResponseStatusDetail">
        <xsd:sequence>
          <xsd:element name="definition" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>
    						The generic definition of subStatusCode.
                        </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="explanation" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>
    						Specific information about the data or circumstances that explains why this status detail is part of the response.
    						It should provide the details aid in troubleshooting and taking further action.
    					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="subStatusCode" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>
    					A numeric value that defines the status detail, like an error code.
    					This code allows client systems to programmatically handle the status detail, if desired.
    		        </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:simpleType name="ResponseStatusCode">
        <xsd:annotation>
          <xsd:documentation>
