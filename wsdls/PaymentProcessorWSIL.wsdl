            </xsd:annotation>
          </xsd:element>
          <xsd:element name="helpText" type="csom:Text" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Help text that is associated with the question</xsd:documentation>
            </xsd:annotation>
