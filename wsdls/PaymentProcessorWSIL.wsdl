      <xsd:complexType name="Header">
        <xsd:annotation>
          <xsd:documentation>
    				Metadata for every POX and SOAP transaction. Data in the Header
    				is used to track and configure
    				service operations. It's supplied
    				by the Sender and returned in the response for continuity and
    				to return values set
    				by the system.
    			</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="sender" type="base:Sender">
            <xsd:annotation>
              <xsd:documentation>
    						A description of the actor that is calling a service operation.
    						This data is supplied by the
    						calling client at the time of the request.
    					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="target" type="base:Target" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>
    						Data that the target system can use to configure the operation.
    					</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="providerId" type="base:ApplicationObjectKey" minOccurs="0"
             maxOccurs="unbounded">
