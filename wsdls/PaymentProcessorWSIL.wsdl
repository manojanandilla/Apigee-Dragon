      <xsd:complexType name="RankedSpecificationValue">
        <xsd:sequence minOccurs="0">
          <xsd:element name="name" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of the Specification value; a label by which the value is commonly known.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="value" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The value of a named property of an object.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ranking" type="csom:Ranking" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The rank or priority of the named value within the specification group.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="dataType" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The data type of the value.  The recognized data types are specified by the xml w3c standard, minus the namespace prefix.  For example xsd:boolean would be expressed as boolean.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="precision" type="xsd:nonNegativeInteger" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>For a numeric value, how many total digits are allowed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="scale" type="xsd:nonNegativeInteger" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>For a numeric value,the number of digits to the right of the decimal point.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="measurementUnit" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The unit of measure of the value, if applicable.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="currencyCode" type="csom:CurrencyCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The currency of the specification value, if the data type is currency.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="Quantity">
        <xsd:simpleContent>
          <xsd:extension base="xsd:decimal">
            <xsd:attribute name="measurementUnit" type="csom:MeasurementUnit">
              <xsd:annotation>
                <xsd:documentation>Unit of quantity</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="Attachment">
        <xsd:annotation>
          <xsd:documentation>A file or file location containing information referenced by the owning business object.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="typeCode" type="csom:AttachmentTypeCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Type of attachment, e.g., URL, TEXT, JPG, PNG, HTML, PDF</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="purposeCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Primary purpose of the attachment, e.g., Shipping, Manufacturing, Invoicing.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="name" type="xsd:normalizedString" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Name of the attachment</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="title" type="xsd:normalizedString" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Title of the attachment. This is not the file name e.g. the title of a document is different from the file name of the document</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="sizeQuantity" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Size of the attachment, usually in bytes.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="creationTime" type="xsd:dateTime" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Date Time of creation of the document in the system of record</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:choice>
            <xsd:element name="embeddedData" type="csom:BinaryObject" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Element holding data embedded directly in the BOD instance</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="characterData" type="xsd:token" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Character data such as embedded data, character data and URI.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="URI" type="xsd:anyURI" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>URI indicating where the attachment is located</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
          <xsd:element name="description" type="csom:Description" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Description of the attachment</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="note" type="csom:Note" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Notes about the object being attached.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="version" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Specifies the version of the object being attached.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="specificationGroup" type="csom:SpecificationGroup"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Specification group for named value pairs.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Uniquely identifies the attachment record.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="StateCriteria">
        <xsd:annotation>
          <xsd:documentation>The criteria requires the state  be in the eligible list or not in the excluded list.</xsd:documentation>
        </xsd:annotation>
        <xsd:choice minOccurs="0">
          <xsd:element name="eligibleState" type="csom:StateCode" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A state that is eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedState" type="csom:StateCode" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A state that is not eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
      </xsd:complexType>
      <xsd:complexType name="CustomerSegmentCriteria">
        <xsd:annotation>
          <xsd:documentation>Customers in the included segments, but not in the excluded segments are eligible.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="eligibleSegment" type="csom:RankedSpecificationValue"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Specifies a customer segmentation attribute name and the value that is eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedSegment" type="csom:RankedSpecificationValue"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Specifies a customer segmentation attribute name and the value that is not eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="DescriptionByChannel">
        <xsd:annotation>
          <xsd:documentation>Description with Sales Channel Code as an attribute.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="csom:Description">
            <xsd:attribute name="salesChannelCode" type="csom:SalesChannelCode">
              <xsd:annotation>
                <xsd:documentation>Sales channel that will be using the description.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="RankedSpecificationGroup">
        <xsd:annotation>
          <xsd:documentation>A ranked specification group captures name value pairs, but adds the ability to rank both the groups and the values in the list.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="groupName" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of the group of specifications.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="groupRanking" type="csom:Ranking" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Rank or priority of the group of specifications.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="specificationValue" type="csom:RankedSpecificationValue"
             minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A named value pair included in the specification group.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="SpecificationValue">
        <xsd:annotation>
          <xsd:documentation>A specification is a property of an object that can be assigned a value.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:token">
            <xsd:attribute name="name" type="xsd:token">
              <xsd:annotation>
                <xsd:documentation>The name of the Specification value; a label by which the value is commonly known.  For example a name = "LongDescription" for a value that contains a long description.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="PostalCodeCriteria">
        <xsd:annotation>
          <xsd:documentation>The criteria requires the postal code be in the eligible list or not in the excluded list.</xsd:documentation>
        </xsd:annotation>
        <xsd:choice minOccurs="0">
          <xsd:element name="eligiblePostalCode" type="csom:PostalCode" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A postal code that is eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedPostalCode" type="csom:PostalCode" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A postal code that is not eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
      </xsd:complexType>
      <xsd:complexType name="CreditWorthinessCriteria">
        <xsd:annotation>
          <xsd:documentation>The criteria requires the credit worthiness be in the eligible list or not in the excluded list.</xsd:documentation>
        </xsd:annotation>
        <xsd:choice minOccurs="0">
          <xsd:element name="eligibleCreditWorthiness" type="xsd:token" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A credit worthiness value that is eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedCreditWorthiness" type="xsd:token" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A credit worthiness value that is not eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
      </xsd:complexType>
      <xsd:complexType name="ChannelCriteria">
        <xsd:annotation>
          <xsd:documentation>The criteria requires the channel be in the eligible list or not in the excluded list.</xsd:documentation>
        </xsd:annotation>
        <xsd:choice minOccurs="0">
          <xsd:element name="eligibleChannel" type="csom:SalesChannelReference"
             minOccurs="0" maxOccurs="unbounded"/>
          <xsd:element name="excludedChannel" type="csom:SalesChannelReference"
             minOccurs="0" maxOccurs="unbounded"/>
        </xsd:choice>
      </xsd:complexType>
      <xsd:complexType name="CityCriteria">
        <xsd:annotation>
          <xsd:documentation>The criteria requires the city be in the eligible list or not in the excluded list.</xsd:documentation>
        </xsd:annotation>
        <xsd:choice minOccurs="0">
          <xsd:element name="eligibleCity" type="csom:CityName" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The name of a city that is eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedCity" type="csom:CityName" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>The name of a city that is excluded.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
      </xsd:complexType>
      <xsd:complexType name="CountyCriteria">
        <xsd:annotation>
          <xsd:documentation>The criteria requires the county be in the eligible list or not in the excluded list.</xsd:documentation>
        </xsd:annotation>
        <xsd:choice minOccurs="0">
          <xsd:element name="eligibleCountyName" type="xsd:token" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A county that is eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedCountyName" type="xsd:token" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>A county that is not eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
      </xsd:complexType>
      <xsd:complexType name="Measure">
        <xsd:simpleContent>
          <xsd:extension base="xsd:decimal">
            <xsd:attribute name="measurementUnit" type="csom:MeasurementUnit">
              <xsd:annotation>
                <xsd:documentation>Unit of measure.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="StoreCriteria">
        <xsd:annotation>
          <xsd:documentation>The criteria requires the store ID to be in the eligible list, or not in the excluded list. If neither list is populated, all stores are eligible.</xsd:documentation>
        </xsd:annotation>
        <xsd:choice minOccurs="0">
          <xsd:element name="eligibleStore" type="csom:StoreId" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>If the store ID is in the eligible list, the store is eligible.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="excludedStore" type="csom:StoreId" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>If the store ID is in the excluded list, the store is excluded from eligibility.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:choice>
      </xsd:complexType>
      <xsd:complexType name="ComplexDescription">
        <xsd:sequence minOccurs="0">
          <xsd:element name="descriptionCode" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A code identifying the description.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="salesChannelCode" type="csom:SalesChannelCode"
             minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A code identifying the sales channel where the description will be used.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="languageCode" type="csom:LanguageCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>A code identifying the language used in the decription text per ISO 639-1 Code: en for English, es for espagnol (Spanish). Reference http://www.iso.org/iso/home/standards/language_codes.htm</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="relativeSize" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The relative size of the description text, as in Short or Long.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:token">
                <xsd:enumeration value="Short"/>
                <xsd:enumeration value="Medium"/>
                <xsd:enumeration value="Long"/>
                <xsd:enumeration value="Abbreviation"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="contentType" type="csom:ContentTypeCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The type of content in the description, as in XML, Text, HTML.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="descriptionText" type="xsd:string" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The textual content of the description.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="BinaryObject">
        <xsd:annotation>
          <xsd:documentation>A set of finite-length sequences of binary octets</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="xsd:base64Binary">
            <xsd:attribute name="characterSetCode" type="xsd:normalizedString">
              <xsd:annotation>
                <xsd:documentation>The character coding used for text in the binary object.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="fileName" type="xsd:normalizedString">
              <xsd:annotation>
                <xsd:documentation>The name of the file to which the binary object belongs.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="mimeCode" type="xsd:normalizedString">
              <xsd:annotation>
                <xsd:documentation>Multipurpose Internet Mail Extensions (MIME) code identifying the content type within the binary object.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
      <xsd:complexType name="DomainKey">
        <xsd:annotation>
          <xsd:documentation>The Domain Key structure is used to uniquely identifiy an object in a system (or domain) other than the system of record.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="keyName" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of the key identifier, within the specified domain. For example, if an object can be identified by a field called code, then keyName = "code".</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="keyValue" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The value of the key identifier.  For example, if an object can be identified by a field called code, then keyName = "code".  The keyValue would be set to the value of the field called "code".</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="additionalKeyPart" type="csom:KeyPart" minOccurs="0"
             maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>Additional key values that are needed to uniquely identify the object within the domain.  For example, if an object can be identified by (code + effectiveDate), the keyName="code" and additionalKeyPart/keyName = "effectiveDate".</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="domainName" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>The system or business domain that is the source of the key. For example: PREPAIDBILLING, WEBCATALOG, SAMSON, SAP.			</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="KeyPart">
        <xsd:annotation>
          <xsd:documentation>Additional key values that are needed to uniquely identify the object within the domain.  For example, if an object can be identified by (code + effectiveDate), the keyName="code" and additionalKeyPart/keyName = "effectiveDate".</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence minOccurs="0">
          <xsd:element name="keyPartName" type="xsd:token" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The name of the additional key part, if a multi-part key is required to uniquely identify an object, as specified in the alternate system (domain), other than the system of record.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="keyPartValue" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>The value of the additional key part, if a multi-part key is required to uniquely identify an object, as specified in the alternate system (domain), other than the system of record.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="ordinal" type="xsd:int">
          <xsd:annotation>
            <xsd:documentation>The ordinal is used to sequence the key parts when accessing the object in the specified domain.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:complexType>
      <xsd:complexType name="LimitRange">
        <xsd:sequence minOccurs="0">
          <xsd:element name="maximumQuantity" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Maximum Quantity on the Limit Range</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="minimumQuantity" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Minimum Quantity on the Limit Range</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="thresholdQuantity" type="csom:Quantity" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Threshold Quantity on the Limit Range</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="Rate">
        <xsd:annotation>
          <xsd:documentation>Numeric information that is assigned or is determined by calculation, counting, or sequencing. It does not require a unit of quantity or unit of measure. Expressed as a quantity per quantity.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
          <xsd:extension base="csom:Quantity">
            <xsd:attribute name="baseMeasurementUnit" type="csom:MeasurementUnit">
              <xsd:annotation>
                <xsd:documentation>The measurement unit of the denominator of the definition of the rate.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="baseValue" type="xsd:decimal">
              <xsd:annotation>
                <xsd:documentation>The value of the denominator of the defintion of the rate.</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:extension>
        </xsd:simpleContent>
      </xsd:complexType>
    </xsd:schema>
  </wsdl11:types>
