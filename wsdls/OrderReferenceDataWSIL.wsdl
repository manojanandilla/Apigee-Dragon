              </xsd:sequence>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="getDomainReferenceDataRequest">
        <xsd:complexType>
          <xsd:complexContent>
