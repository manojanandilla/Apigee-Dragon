          <xsd:complexContent>
            <xsd:extension base="base:BaseResponse">
              <xsd:sequence>
                <xsd:element ref="csom:serviceRequest" minOccurs="0"/>
              </xsd:sequence>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="updateCaseRequest">
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="base:BaseRequest">
              <xsd:sequence>
                <xsd:element name="assignedTo" type="xsd:token" minOccurs="0"/>
                <xsd:element name="caseId" type="xsd:token"/>
                <xsd:element name="notes" type="csom:Note" minOccurs="0"/>
                <xsd:element name="status" type="xsd:token" minOccurs="0"/>
                <xsd:element name="dueDate" type="xsd:dateTime" minOccurs="0"/>
                <xsd:element name="reason" type="xsd:token" minOccurs="0"/>
                <xsd:element name="adjustmentAmount" type="csom:Amount" minOccurs="0"/>
                <xsd:element name="attachment" type="csom:Attachment" minOccurs="0"/>
              </xsd:sequence>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="getAllowedAdjustmentLimitResponse">
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="base:BaseResponse">
              <xsd:sequence>
                <xsd:element name="adjustmentCodes" minOccurs="0" maxOccurs="unbounded">
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="adjustmentCode" type="xsd:token" minOccurs="0"/>
                      <xsd:element name="amount" type="csom:Amount" minOccurs="0"/>
