                <xsd:element name="dueToDate" type="xsd:dateTime" minOccurs="0"/>
                <xsd:element name="dueFromDate" type="xsd:dateTime" minOccurs="0"/>
                <xsd:element name="createdBy" type="xsd:normalizedString" minOccurs="0"/>
                <xsd:element ref="csom:financialAccountNumber" minOccurs="0"/>
                <xsd:element ref="csom:MSISDN" minOccurs="0"/>
                <xsd:element name="assignedTo" type="xsd:normalizedString" minOccurs="0"/>
