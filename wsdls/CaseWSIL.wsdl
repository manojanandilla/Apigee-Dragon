                <xsd:element name="status" type="csom:ServiceRequestStatus"
                   minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element name="transactionCode" type="xsd:token" minOccurs="0"/>
                <xsd:element name="creationToDate" type="xsd:dateTime" minOccurs="0"/>
                <xsd:element name="creationFromDate" type="xsd:dateTime" minOccurs="0"/>
                <xsd:element name="storeId" type="csom:StoreId" minOccurs="0"
