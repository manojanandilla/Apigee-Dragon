      <xsd:complexType name="ResponseStatusDetail">
        <xsd:sequence>
          <xsd:element name="definition" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>
    						The generic definition of subStatusCode.
                        </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="explanation" type="xsd:normalizedString" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>
